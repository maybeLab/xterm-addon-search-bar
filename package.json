{
  "name": "xterm-addon-search-bar",
  "version": "0.1.2",
  "global": "SearchBarAddon",
  "main": "lib/xterm-addon-search-bar.js",
  "module": "lib/xterm-addon-search-bar.esm.js",
  "unpkg": "lib/xterm-addon-search-bar.min.js",
  "types": "typings/index.d.ts",
  "keywords": [
    "rollup",
    "babel",
    "starter",
    "template",
    "cli"
  ],
  "author": "yinshuxun",
  "url": "https://github.com/yinshuxun/xterm-addon-search-bar",
  "license": "MIT",
  "scripts": {
    "cz": "git-cz",
    "clean": "rimraf lib",
    "start": "yarn run clean && cross-env NODE_ENV=development rollup -w -c scripts/rollup.config.dev.js",
    "build": "yarn run clean && cross-env NODE_ENV=production rollup -c scripts/rollup.config.prod.js",
    "test": "npm run flow && jest",
    "flow": "flow check",
    "serve": "serve -p 7000",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "files": [
    "src",
    "lib/*.js"
  ],
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "rxjs-compat": "^6.5.4",
    "xterm": "^4.3.0",
    "xterm-addon-search": "^0.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "ansi_up": "^4.0.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "commitizen": "^4.0.3",
    "commitlint-config-cz": "^0.12.1",
    "cross-env": "^6.0.3",
    "cz-customizable": "^5.2.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.62.0",
    "husky": "^3.1.0",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "monaco-editor": "^0.19.0",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^2.0.6",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-typescript2": "^0.26.0",
    "rollup-plugin-uglify": "^2.0.1",
    "serve": "^10.0.2",
    "standard-version": "^4.3.0",
    "typescript": "^3.7.4",
    "uglify-es": "^3.3.4",
    "xterm-addon-web-links": "^0.2.1",
    "xterm-addon-webgl": "^0.4.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
